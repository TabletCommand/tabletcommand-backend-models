// cSpell Settings
{
  // Version of the setting file.  Always 0.1
  "version": "0.2",
  // language - current active spelling language
  "language": "en-US",
  // words - list of words to be always considered correct
  "words": [
    "2dsphere",
    "apikey",
    "arcgis",
    "backend",
    "bson",
    "Counti",
    "DDTHH",
    "Enroute",
    "Esri",
    "fdid",
    "firefront",
    "firemapper",
    "firemap",
    "flushall",
    "ipad",
    "iphone",
    "minlength",
    "mockgoose",
    "parens",
    "Postdirectional",
    "Predirectional",
    "pubnub",
    "quotemark",
    "signup",
    "tabletcmd",
    "tabletcommand",
    "Ursache",
    "virtuals",
    "zonehaven"
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  // For example "hte" should be "the"
  "flagWords": [
    "hte"
  ],
  "ignoreWords": [],
  "languageSettings": [{
    "languageId": "*",
    "dictionaries": []
  }],
  "files": [
    // "README.md",
    ".buildkite/**",
    "src/**"
    // "test/**"
  ],
  "ignorePaths": [
    "node_modules",
    "build",
    "src/test",
    "/project-words.txt",
  ]
}
